{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.4.1",
      "destination": "wwwroot/js/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.js"
      ]
    },
    {
      "library": "bootstrap@4.4.1",
      "destination": "wwwroot/js/bootstrap/",
      "files": [
        "dist/js/bootstrap.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "aes-js@3.1.2",
      "destination": "wwwroot/js/aes-js/"
    }
  ]
}